{
  "codeLens.enable": true,
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "trace.server": "verbose",
      "filetypes": [
        "go"
      ]
    },
    "php": {
      "command": "intelephense",
      "args": [
        "--stdio"
      ],
      "filetypes": [
        "php"
      ],
      "trace.server": "verbose",
      "initializationOptions": {
        "storagePath": "/Users/alinademi/.config/coc/extensions/@yaegassy/coc-intelephense-data",
        "stubs": [
          "/Users/alinademi/.composer/vendor/php-stubs"
        ]
      }
    }
  }
}